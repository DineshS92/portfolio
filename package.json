{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=\"-r esm\" gatsby build",
    "develop": "cross-env NODE_OPTIONS=\"-r esm\" gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "netlify": "netlify dev",
    "prebuild": "netlify-lambda install"
  },
  "dependencies": {
    "@u-wave/react-youtube": "^0.7.1",
    "babel-plugin-styled-components": "^1.11.1",
    "dotenv": "^8.2.0",
    "gatsby": "^2.24.63",
    "gatsby-image": "^2.4.20",
    "gatsby-plugin-react-helmet": "^3.3.12",
    "gatsby-plugin-sharp": "^2.6.37",
    "gatsby-plugin-styled-components": "^3.3.12",
    "gatsby-source-filesystem": "^2.3.31",
    "gatsby-transformer-json": "^2.4.13",
    "gatsby-transformer-sharp": "^2.5.16",
    "isomorphic-fetch": "^3.0.0",
    "netlify-lambda": "^2.0.1",
    "normalize.css": "^8.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^3.11.0",
    "react-typical": "^0.1.3",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "prettier": "^2.1.1",
    "esm": "^3.2.25"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  }
}
